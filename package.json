        "eslint-plugin-react-refresh": "^0.4.5",
        "husky": "^8.0.3",
        "is-ci": "^3.0.1",
        "lint-staged": "^15.2.1",
        "postcss": "^8.4.33",
        "prettier": "^3.2.5",
