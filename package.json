{
    "name": "uparkt-admin",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "lint-staged": {
        "*.(js|jsx|ts|tsx)": [
            "npm run lint:write",
            "npm run format"
        ]
    },
    "scripts": {
        "dev": "vite",
        "build": "tsr generate && tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
        "lint:write": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/**",
        "prepare": "is-ci || husky install"
    },
