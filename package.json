        "@tanstack/react-router": "^1.15.17",
        "@tanstack/router-cli": "^1.15.10",
        "@tanstack/router-vite-plugin": "^1.15.11",
        "axios": "^1.6.7",
        "emoji-mart": "^5.5.2",
        "framer-motion": "^10.18.0",
        "i18next": "^23.8.2",
        "js-md5": "^0.8.3",
        "localforage": "^1.10.0",
        "luxon": "^3.4.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.50.0",
